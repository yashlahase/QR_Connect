{
  "expo": {
    "name": "QRConnect",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yashlahase79.QRConnect"
      
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.yashlahase79.QRConnect"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "986be3d1-5eb6-473b-a0df-48c71119281d"
      }
    }
  }
}
